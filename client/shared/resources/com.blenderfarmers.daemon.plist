<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN
http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
<dict>
     <key>Label</key>
     <string>com.blenderfarmers.daemon</string>
     <key>ProgramArguments</key>
     <array>
          <string>/Applications/BlenderFarmers.app/Contents/MacOS/BlenderFarmers</string>
          <string>--background</string>
     </array>
     <key>KeepAlive</key>
     <true/>
     <key>RunAtLoad</key>
     <true/>
     <key>StandardOutPath</key>
     <string>/var/log/blenderfarmers.log</string>
     <key>StandardErrorPath</key>
     <string>/var/log/blenderfarmers.log</string>
     <key>AbandonProcessGroup</key>
     <true/>
</dict>
</plist>
